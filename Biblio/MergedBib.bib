	Sentelink = {file://localhost/Users/lebourlot/THESE/BIBLIOGRAPHY/PDFS/These/van%20de%20Wolfshaar%202008%20Ecology%20Population%20feedback.pdf,Sente,},
	Title = {Population feedback after successful invasion leads to ecological suicide in seasonal environments.},
