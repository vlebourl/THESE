	Organization = {Department of Evolutionary Studies of Biosystems, The Graduate University for Advanced Studies (Sokendai), Hayama, 240-0193, Kanagawa, Japan, sasaki\_akira\@soken.ac.jp.},
	Pubmedid = {21103990},
	Sentelink = {file://localhost/Users/lebourlot/THESE/BIBLIOGRAPHY/PDFS/These/Sasaki%202010%20J%20Math%20Biol%20Oligomorphic%20dynamics%20for.pdf,Sente,},
	Title = {Oligomorphic dynamics for analyzing the quantitative genetics of adaptive speciation.},
